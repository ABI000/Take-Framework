{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "DBSettings": {
    "ConnectionString": "server=.;uid=sa;pwd=abi@123456;database=SampleWeb;TrustServerCertificate=True"
  },
  "LocalizationOptions": {
    "StorageType": "Cache",
    "Storage": "",
    "DefaultLanguageCode": "en-US"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "JwtConfiguration": {
    "SecretKey": "1234567890qwertyuiopasdfghjklzxcvbnm",
    "Expires": 3000
  }
}
